    <owl:Class rdf:about="http://www.bigcat.unimaas.nl/sbd4nano/gracious.owl#GR_00000163">
        <rdfs:label xml:lang="en">PC_ZETA_POTENTIAL</rdfs:label>
        <obo:IAO_0000115>IUCLID 4.29: Zeta potential
OHT 106: Nanomaterial Zeta potential</obo:IAO_0000115>
